<h1 id="api_key" data-api_key="<%= APP_CONFIG['api_key'] %>">
  <%= image_tag(@source.name + ".png", :size => "16x16") if FileTest.exist?("#{Rails.root.to_s}/app/assets/images/#{@source.name}.png") %>
  <%= link_to h(@source.display_name), admin_source_path(@source) %> <small class=\"hidden-phone\"><%= h(@source.human_state_name) %></small>
</h1>
